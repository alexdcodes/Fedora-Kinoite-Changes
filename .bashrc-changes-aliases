# bash aliases n changes 

alias d="cd ~/Downloads"
alias s="cd ~/Scripts"
alias p="cd ~/Pictures"
alias ds="cd ~/Documents"
alias dev="cd ~/Development"
alias m="cd ~/Music"
alias l="ls -la"
alias c="cd"
alias pds="podman stats"

